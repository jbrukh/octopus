{{#if model.isConnected}}
  {{#linkTo 'status.index' class="btn btn-success"}}
    <i class="icon-thumbs-up"></i>&nbsp;&nbsp;{{model.device_name}}
  {{/linkTo}}
{{else}}
  {{#linkTo 'status.index' class="btn btn-warning"}}
    <i class="icon-thumbs-down"></i>&nbsp;Octopus Connector
  {{/linkTo}}
{{/if}}